{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "email": {
      "type": "email",
      "private": true,
      "required": true
    },
    "time": {
      "type": "integer",
      "private": false,
      "required": true
    },
    "progress": {
      "type": "decimal",
      "default": 1,
      "private": false,
      "required": true,
      "max": 1,
      "min": 0
    },
    "name": {
      "type": "string",
      "required": true,
      "private": true
    },
    "key": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "integer",
      "private": true,
      "required": true,
      "min": 0
    },
    "description": {
      "type": "text",
      "private": true,
      "required": true
    },
    "stripePaymentId": {
      "type": "string",
      "private": true,
      "required": true
    },
    "fullName": {
      "type": "string",
      "required": true,
      "private": true
    },
    "successUrl": {
      "type": "string",
      "required": true,
      "private": true
    },
    "tax": {
      "required": true,
      "default": 0,
      "private": true,
      "type": "integer"
    },
    "invoiceId": {
      "type": "string",
      "private": true
    }
  }
}
